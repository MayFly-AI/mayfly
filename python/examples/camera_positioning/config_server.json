{
    "id":"example_server",
    "services":[
        {
            "type":"videoServer",
            "id":"camera_positioning",
            "errorCorrection":{
                "type":"arq"
            },
            "transfer":{
                "protocol":"udp",
                "bindAddress":{
                    "ip":"0.0.0.0",
                    "port":8999
                }
            },
            "sources":[
                {
                    "id":1,
                    "type":"camera",
                    "clock":30,
                    "mode":"640x480"
                },
                {
                    "id":2,
                    "type":"imu",
                    "clockDivider":1,
                    "frequency":25
                },
                {
                    "id":3,
                    "type":"mag",
                    "clockDivider":1,
                    "frequency":25
                },
                {
                    "id":4,
                    "type":"tag",
                    "clockDivider":3
                }
            ]
        }
    ]
}
